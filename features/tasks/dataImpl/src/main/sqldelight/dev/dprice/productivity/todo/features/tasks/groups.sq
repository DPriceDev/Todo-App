import dev.dprice.productivity.todo.features.tasks.data.model.Colour;
import dev.dprice.productivity.todo.features.tasks.data.model.Icon;

CREATE TABLE groupItem (
    id TEXT NOT NULL PRIMARY KEY,
    title TEXT NOT NULL,
    colour TEXT AS Colour NOT NULL,
    icon TEXT AS Icon NOT NULL
);

CREATE INDEX group_id ON groupItem(id);

selectAll:
SELECT *
FROM groupItem;

insert:
INSERT OR REPLACE INTO groupItem(id, title, colour, icon)
VALUES (?, ?, ?, ?);

delete:
DELETE FROM groupItem WHERE id = ?;